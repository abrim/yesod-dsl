toDefaultFilter~{entityName e} :: FilterJsonMsg -> Maybe (Filter ~{entityName e})
toDefaultFilter~{entityName e} f = case (filterJsonMsg_field f) of
~{fieldFilters}
    _ -> Nothing

toDefaultSort~{entityName e} :: SortJsonMsg -> Maybe (SelectOpt ~{entityName e})
toDefaultSort~{entityName e} s = case (sortJsonMsg_property s) of
~{fieldSorters}
    _ -> Nothing

