get~{entityName e}ManyR :: Handler RepJson
get~{entityName e}ManyR = do
~{unlines $ indent $ maybeRequireAuth params}
~{unlines $ indent $ genFilters e params}
~{unlines $ indent $ genSelectOpts e params}
    entities <- runDB $ selectList (concat filters) (concat selectOpts)
~{unlines $ indent $ postHooks " entities" params}
    jsonToRepJson $ object [ "entities" .= toJSON entities ]    
