                "~{fieldName f}" -> case (fromPathPiece $ filterJsonMsg_value fjm) of 
                    (Just v) -> where_ $ defaultFilterOp (filterJsonMsg_comparison fjm) (~{vn} ~{projectField isMaybe} ~{entityFieldTypeName e f}) ~{makeJust (fieldOptional f || isMaybe) "(val v)"} 
                    _        -> return ()
