import "module.dbdef";

entity Bar {
    string name (default "foo", unique, check longer3);
    Foo foo (bar);
    unique index (name);
}
entity Foo {
    implements Versioned;
    optional string name;
}

interface Versioned {
    integer version;
}
relation FooBar {
    Foo foo ([] fooBars);
    Bar bar (bar);
}
    
