import "module.dbdef";

entity Bar {
    Text name (default "foo", unique, check longer3);
    Foo foo (bar);
    unique index (name);
}
entity Foo {
    implements Versioned;
    optional Text name;
}

interface Versioned {
    Int64 version;
}


relation FooBar {
    Foo foo (fooBars);
    Bar bar (bar);
}
    
